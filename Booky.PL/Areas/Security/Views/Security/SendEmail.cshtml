@model ForgetPasswordViews

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_SecurityLayout.cshtml";
}
<div class="wrapper">
<h2>Forget Password</h2>
<form asp-action="SendEmail" method="post">
    <div class="input-box">
        <input asp-for="Email" type="text" placeholder="Enter your Email" required>
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="input-box button">
        <button type="submit" class="btn btn-primary form-control">Send Email </button>
    </div>
</form>
</div>
